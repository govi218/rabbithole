{
  "manifest_version": 2,
  "name": "Rabbit Hole",
  "author": "Ousmane",
  "version": "1.0.1",
  "description": "Nibble. Nibble. Nib. ",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128":"icons/128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "history"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}